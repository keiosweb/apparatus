{
    "name": "keios/apparatus",
    "description": "Event-based business logic processor",
    "keywords": [
        "keios",
        "business logic",
        "apparatus"
    ],
    "authors": [
        {
            "name": "Łukasz Biały",
            "email": "lukasz@c-call.eu",
            "homepage": "http://keios.eu",
            "role": "Developer"
        }
    ],
    "require-dev":{
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "0.9.*",
        "scrutinizer/ocular": "~1.1"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Keios\\Apparatus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Keios\\Apparatus\\Tests\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
